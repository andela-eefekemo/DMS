{
  "name": "document-management-system",
  "version": "1.0.0",
  "engines": {
    "node": "6.10.2",
    "npm": "3.10.10"
  },
  "description": "Creating a Document Management System where each document defines access rights; the document defines which roles can access it.",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "NODE_ENV=production babel-node -- index.js",
    "test": "npm run setup:test && NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register ./server/test/**/*.js --timeout 2000000",
    "dev": "NODE_ENV=development nodemon --watch server --exec babel-node -- index.js",
    "db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "setup:test": "npm run db:migrate:test && NODE_ENV=test sequelize db:seed:all",
    "build": "webpack --config ./webpack.config.prod.js --progress --colors --watch",
    "db:seed": "sequelize db:seed:all",
    "client:test": "jest --coverage client/test",
    "client:test:update": "jest --updateSnapshot client/test",
    "client:test:e2e": "./node_modules/.bin/nightwatch"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "globals": {
      "window": true
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ],
    "setupFiles": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-eefekemo/DMS.git"
  },
  "keywords": [
    "Express",
    "PostGreSQL"
  ],
  "author": "Eguonoghene Efekemo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-eefekemo/DMS/issues"
  },
  "homepage": "https://github.com/andela-eefekemo/DMS#readme",
  "devDependencies": {
    "babel-jest": "^25.1.0",
    "chai": "^4.0.2",
    "coveralls": "^3.0.9",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.0.1",
    "faker": "^4.1.0",
    "jest": "^25.1.0",
    "mocha": "^7.0.1",
    "nightwatch": "^1.3.4",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.17.2",
    "chai-http": "^4.3.0",
    "colors": "^1.1.2",
    "compression": "^1.7.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.4.4",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.3",
    "express-validator": "^3.2.0",
    "faker": "^4.1.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^0.4.5",
    "jest-cli": "^25.1.0",
    "jquery": "^3.2.1",
    "jsdom": "^16.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.4",
    "mini-css-extract-plugin": "^0.9.0",
    "morgan": "^1.8.2",
    "moxios": "^0.4.0",
    "node-sass": "^4.5.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-html-parser": "^2.0.2",
    "react-materialize": "^3.5.9",
    "react-paginate": "^6.3.2",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
