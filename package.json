{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "Creating a Document Management System where each document defines access rights; the document defines which roles can access it.",
  "main": "server.js",
  "scripts": {
    "test": "npm run setup:test && NODE_ENV=test mocha --compilers js:babel-core/register ./server/test/**/*.js",
    "dev": "babel-node -- bin/www",
    "db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "setup:test": "npm run db:migrate:test && NODE_ENV=test sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-eefekemo/DMS.git"
  },
  "keywords": [
    "Express",
    "PostGreSQL"
  ],
  "author": "Eguonoghene Efekemo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-eefekemo/DMS/issues"
  },
  "homepage": "https://github.com/andela-eefekemo/DMS#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "mocha": "^3.4.2"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "chai-http": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-validator": "^3.2.0",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pg": "^6.2.4",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.23.0",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
